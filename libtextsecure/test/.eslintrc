{
  "env": {
    "browser": true,
    "node": false,
    "mocha": true,
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "strict": "off",
    "more/no-then": "off",
  },
  "globals": {
    "assert": true,
    "assertEqualArrayBuffers": true,
    "dcodeIO": true,
    "getString": true,
    "hexToArrayBuffer": true,
    "PROTO_ROOT": true,
    "stringToArrayBuffer": true,
  }
}